{
  "name": "elm-review-rules",
  "version": "1.0.0",
  "description": "My personal elm-review config for all Elm projects.",
  "scripts": {
    "test": "npm-run-all --print-name --silent --sequential test:make test:format",
    "test:make": "elm make src/ReviewConfig.elm",
    "test:format": "elm-format src/ --validate",
    "postinstall": "elm-tooling install"
  },
  "dependencies": {
    "elm-doc-preview": "^5.0.5",
    "elm-review": "^2.9.1",
    "elm-tooling": "^1.12.0",
    "fs-extra": "11.1.0",
    "glob": "8.1.0",
    "npm-run-all": "^4.1.5"
  }
}
